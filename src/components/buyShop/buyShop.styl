.shop-detial
  	padding: 10px
  	box-sizing: border-box
  	background-color: #fff
  	.store-name
      font-size: 14px
      color: #000
      padding: 10px 0
      border-bottom: 1px solid rgba(7, 17, 27, 0.1)
      img, .text
        display: inline-block
        vertical-align: middle
    .store-goods
      border-bottom: 1px solid rgba(7, 17, 27, 0.1)
      .price-content
      	font-size: 0
      	padding: 10px 4px
      	position: relative
      	.name
      	  display: inline-block
      	  font-size: 14px
      	  color: #666
	    .price-count
	      position: absolute
	      right: 10px
	      display: inline-block
	      font-size: 14px
	      .count
	      	color: #999
	      	position: absolute
	      	right: 50px
    .delivery
      margin-top: 10px
      padding-left: 4px
      position: relative
      .delivery-price
        font-size: 14px
      .price
      	position: absolute
      	right: 10px