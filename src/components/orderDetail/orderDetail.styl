.orderDetail
  width: 100%
  height: 100%
  position: absolute
  z-index: 160
  top: 0
  left: 0
  overflow: hidden
  background-color: #fff
  transform: translate3d(0, 0, 0)
  &.move-enter-active, &.move-leave-active
    transition: all 0.2s linear
  &.move-enter, &.move-leave-to
    transform: translate3d(100%, 0, 0)
  .orderDetail-content
    width: 100%
    position: absolute
    z-index: 110
    top: 34px
    left: 0
    bottom: 0
    overflow: hidden