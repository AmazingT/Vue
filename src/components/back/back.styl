  .head
    width: 100%
    height: 34px
    line-height: 34px
    background-color: #0089dc
    color: #fff
    text-align: center
    position: fixed
    z-index: 120
    top: 0
    left: 0
    .back, .title
      display: inline-block
    .back
      position: absolute
      left: 4px
      padding: 10px
      line-height: 12px
      transform: rotate(180deg)
      .icon-zuoyoujiantou
        font-size: 25px